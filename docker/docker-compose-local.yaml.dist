services:
  app:
  # descomentar cuando se necesite usar el entorno dev (esto es para compilar, se puede reemplazar por una im√°gen precompilada)
    build: 
      context: ./app/dev
    volumes:
    # descomentar cuando se necesite usar el entorno dev
      - ./app/dev/php.ini:/usr/local/etc/php/php.ini
